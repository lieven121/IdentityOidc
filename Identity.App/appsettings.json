{
  "Hosting": {
    "ReverseProxySupport": true,
    "AssumeEveryRequestHttps": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenIddict": {
    "OnlyAllowHttps": true,
    "Encryption": {
      "Key": null,
      "Cert": {
        "Path": "../../identity-encryption.pfx",
        "GenerateIfEmpty": true,
        "Password": null,
        "ValidityMonths": 36

      }
    },
    "Signing": {
      "Key": null,
      "Cert": {
        "Path": "../../identity-signing.pfx",
        "GenerateIfEmpty": true,
        "Password": null,
        "ValidityMonths": 36
      }
    },

    "ApplicationConfigs": [],
    "Users": [
      {
        "Username": null,
        "Email": null,
        "Password": null
      }
    ]
  }
}
